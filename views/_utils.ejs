<div class="buttons-wrapper">
    <a href="/" class="logo"><%= title %></a>
    <div class="buttons">
        <a href="/new" class="btn">New</a>
        <% if (locals.canSave) { %>
        <button class="btn" type="submit">Save</button>
        <% } %>
        <% if (locals.id != null) { %>
        <a href="/code/<%= locals.id %>/duplicate" class="btn">Duplicate</a>
        <% } %>
    </div>
</div>